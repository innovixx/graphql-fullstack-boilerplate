{
	"name": "graphql-fullstack-boilerplate",
	"dockerComposeFile": "docker-compose.yml",
	"service": "server",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"customizations": {
		"codespaces": {
			"repositories": {
				"Innovixx-Development/graphql-server-boilerplate": {
					"permissions": "read-all"
				},
				"Innovixx-Development/graphql-client-boilerplate": {
					"permissions": "read-all"
				}
			}
		},
		"vscode": {
			"extensions": [
				"Prisma.prisma",
				"GraphQL.vscode-graphql",
				"dbaeumer.vscode-eslint",
				"orsenkucher.vscode-graphql",
				"stylelint.vscode-stylelint"
			]
		}
	},
	"forwardPorts": [
		3000,
		9000,
		5432,
		19000
	]
}